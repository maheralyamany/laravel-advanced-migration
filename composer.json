{
    "name": "maheralyamany/laravel-advanced-migration",
    "description": "Generate migrations from existing database structures",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Maher Alyamany",
            "email": "maheralyamany4@gmail.com"
        }
    ],
    "keywords": [
        "Maher Alyamany",
        "MaherAlyamany",
        "maheralyamany",
        "migration generator",
        "migration-generator",
        "framework",
        "laravel"
    ],
    "support": {
        "issues": "https://github.com/maheralyamany/laravel-advanced-migration/issues",
        "source": "https://github.com/maheralyamany/laravel-advanced-migration"
    },
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/config": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "marcj/topsort": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.17|^8.0|^9.0|^10.0",
        "laravel/pint": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "AdvancedMigration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "test": [
            "vendor/bin/phpunit"
        ],
        "lint": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdvancedMigration\\MigrationGeneratorProvider"
            ]
        }
    },
    "prefer-stable": true
}
